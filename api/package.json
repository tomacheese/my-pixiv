{
  "name": "my-pixiv-api",
  "version": "1.0.0",
  "description": "pixiv client for myself",
  "main": "dist/main.js",
  "repository": "git@github.com:tomacheese/my-pixiv.git",
  "author": "Tomachi",
  "private": true,
  "scripts": {
    "build": "ts-node -r tsconfig-paths/register ./src/main.ts",
    "build-test": "ts-node -r tsconfig-paths/register ./src/test.ts",
    "dev": "ts-node-dev -r tsconfig-paths/register ./src/main.ts",
    "start": "node ./dist/main.js",
    "compile": "tsc -p .",
    "compile:test": "tsc -p . --noEmit",
    "lint": "run-p -c lint:prettier lint:eslint lint:tsc",
    "lint:prettier": "prettier --check src",
    "lint:eslint": "eslint . --ext ts,tsx",
    "lint:tsc": "tsc",
    "lintfix": "run-s fix",
    "fix": "run-s fix:prettier fix:eslint",
    "fix:eslint": "eslint . --ext ts,tsx --fix",
    "fix:prettier": "prettier --write src"
  },
  "devDependencies": {
    "@types/js-yaml": "4.0.5",
    "@types/node": "18.11.12",
    "@types/qs": "6.9.7",
    "@types/ws": "8.5.3",
    "@typescript-eslint/eslint-plugin": "5.38.0",
    "@typescript-eslint/parser": "5.38.0",
    "axios": "0.27.2",
    "eslint": "8.23.1",
    "eslint-config-prettier": "8.5.0",
    "eslint-config-standard": "17.0.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-n": "15.2.5",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "6.0.1",
    "my-pixiv-types": "*",
    "prettier": "2.7.1",
    "qs": "6.11.0",
    "ts-node": "10.9.1",
    "ts-node-dev": "2.0.0",
    "typescript": "4.9.4",
    "yarn-run-all": "3.1.1"
  },
  "dependencies": {
    "@fastify/websocket": "7.1.1",
    "fastify": "4.10.2"
  }
}
